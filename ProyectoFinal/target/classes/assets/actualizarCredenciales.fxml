                  <TextField fx:id="confirmarUsuarioTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <PasswordField fx:id="confirmarContrasenaPasswordField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
            <Button fx:id="actualizarButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="125.0" text="Actualizar" />
         </children>
         <padding>
            <Insets top="100.0" />
         </padding></VBox>
   </children>
